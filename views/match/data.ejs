<%- include("../partials/header") %>

<h1>MATCH ADDED</h1>

<a href="/match">Go back</a>

<div class="row">
  
  <div class="col-md-6">
    <h2 class="sub-header">Points</h2>
    <div class="table-responsive">
      <table class="table table-striped table-bordered">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Set Score</th>
            <th scope="col">Game Score</th>
            <th scope="col">Point Score</th>
            <th scope="col">Server</th>
            <th scope="col">Side</th>
            <th scope="col">Rally Length</th>
            <th scope="col">Result</th>
            <th scope="col">Winner</th>
          </tr>
        </thead>
        <tbody>
        <% points.forEach(point => { %>
        <tr>
                <th scope="row"><%= point.point %></th>
                <td><%= point.setScore %></td>
                <td><%= point.gameScore %></td>
                <td><%= point.pointScore %></td>
                <td><%= point.server %></td>
                <td><%= point.side %></td>
                <td><%= point.rallyLength %></td>
                <td><%= point.result %></td>
                <td><%= point.winner %></td>
        </tr>
        <% }) %>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
  
  <div class="col-md-6">
    <h2 class="sub-header">Shots</h2>
    <div class="table-responsive">
      <table class="table table-striped table-bordered">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Set Score</th>
            <th scope="col">Game Score</th>
            <th scope="col">Point Score</th>
            <th scope="col">Server</th>
            <th scope="col">Side</th>
            <th scope="col">Rally Length</th>
            <th scope="col">Result</th>
            <th scope="col">Winner</th>
          </tr>
        </thead>
        <tbody>
        <% points.forEach(point => { %>
        <tr>
                <th scope="row"><%= point.point %></th>
                <td><%= point.setScore %></td>
                <td><%= point.gameScore %></td>
                <td><%= point.pointScore %></td>
                <td><%= point.server %></td>
                <td><%= point.side %></td>
                <td><%= point.rallyLength %></td>
                <td><%= point.result %></td>
                <td><%= point.winner %></td>
        </tr>
        <% }) %>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
  
</div>



<%- include("../partials/footer") %>


